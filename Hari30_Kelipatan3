package Day9;

import java.util.Scanner;

public class Hari30 {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("masukkan angka : ");
        int n = scanner.nextInt();

        int i = 3;
        System.out.println(" ");

        while (i <= n) {
            if (i % 3 == 0) {
                System.out.println(i);

            }
            i++;
        }
        scanner.close();

    }

}


