
package Day9;

import java.util.Scanner;

public class Day25 {
    public static void main(String[] args) {
        Scanner sc = new Scanner (System.in);
        
        System.out.println("masukkan tugas anda :");
        int a = sc.nextInt();
        
        for (int i = 1; i <= 100; i++) {
            System.out.println("tugas ke-" + i);
        }
    }
}

    
