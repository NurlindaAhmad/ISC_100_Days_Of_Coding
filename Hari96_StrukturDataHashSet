import java.util.HashSet;

public class Main {
	public static void main(String[] args) {

        HashSet<String> hashSet = new HashSet<>();

        hashSet.add("Mobil");
        hashSet.add("Motor");
        hashSet.add("Sepeda");

 
        System.out.println("HashSet awal: " + hashSet);

        hashSet.remove("Motor");
        System.out.println("HashSet setelah penghapusan: " + hashSet);

        if (hashSet.contains("Mobil")) {
            System.out.println("Mobil ada dalam HashSet");
        } else {
            System.out.println("Mobil tidak ada dalam HashSet");
        }
        
        System.out.println("Ukuran HashSet: " + hashSet.size());
    }
}
