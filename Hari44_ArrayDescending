import java.util.Arrays;
public class Main {
	public static void main(String[] args) {
		double [ ] data = { 4, 3.1, 3, 9, 0.1, 4.2, 2, 1, 7, 9} ;
		
		Arrays.sort(data);
		double [ ] nilaiData = new double [data.length];
		for (int i = 0; i < data.length; i++){
			nilaiData [ i ] = data[data.length - 1 - i];
	}
	System.out.println(Arrays.toString(nilaiData));
}
}
