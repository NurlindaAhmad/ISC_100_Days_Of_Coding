import java.util.ArrayList;
public class Main {
	public static void main(String[] args) {
    ArrayList<String> hewan = new ArrayList<String>();
    hewan.add("Singa");
    hewan.add("Gajah");
    hewan.add("Harimau");
    hewan.add("Jerapah");
    
    for (int i = 0; i < hewan.size(); i++) {
      System.out.println(hewan.get(i));
    }
  }
}

	
