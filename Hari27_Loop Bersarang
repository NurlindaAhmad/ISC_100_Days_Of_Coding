package Day9;

public class Hari27 {

    public static void main(String[] args) {

        int jumlahBintang = 4;

        for (int a = 1; a <= jumlahBintang; a++) {
            for (int j = 0; j < a; j++) {
                System.out.print("*");

            }
            System.out.println("");

        }

    }

}
